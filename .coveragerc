[run]
# Coverage configuration
source = src
omit =
    */tests/*
    */ui/*
    */__pycache__/*
    */site-packages/*

[report]
# Reporting options
precision = 2
show_missing = True
skip_covered = False

exclude_lines =
    # Exclude lines from coverage
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

[html]
directory = htmlcov
